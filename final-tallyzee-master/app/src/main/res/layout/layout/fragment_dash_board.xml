<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/white"
    android:scrollbars="vertical"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".ui.main.ListDashBoardFragment.DashBoardFragment">
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <RelativeLayout
            android:id="@+id/hadder"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="60sp"
                android:textAlignment="center"
                android:layout_centerHorizontal="true"
                android:text="register"
                android:fontFamily="@font/avenirltstd_medium"/>
        </RelativeLayout>
        <RelativeLayout
            android:fitsSystemWindows="true"
            android:layout_width="match_parent"
            android:layout_height="400dp">

            <RelativeLayout
                android:layout_width="120dp"
                android:layout_height="110dp"
                android:background="@drawable/ic_hecta"
                android:layout_centerVertical="true"
                android:layout_centerInParent="true"
                android:backgroundTint="#0B5345"
                android:id="@+id/relativeLayout3">
                <ImageView
                    android:id="@+id/test"
                    android:layout_width="50dp"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="10dp"
                    app:srcCompat="@drawable/ic_user"
                    android:layout_height="50dp" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="4dp"
                    android:layout_centerVertical="true"
                    android:layout_below="@id/test"
                    android:textSize="12sp"
                    android:text="Profit and Loss"
                    android:layout_marginLeft="30dp" />
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="120dp"
                android:layout_height="110dp"

                android:layout_alignTop="@+id/relativeLayout3"
                android:layout_marginTop="54dp"
                android:backgroundTint="#16A085"
                android:layout_marginEnd="36dp"
                android:background="@drawable/ic_hecta"
                android:id="@+id/relativeLayout5"

                android:layout_alignParentRight="true"
                android:layout_marginRight="40dp">
                <ImageView
                    android:id="@+id/test1"
                    android:layout_width="50dp"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="10dp"
                    app:srcCompat="@drawable/ic_star"
                    android:layout_height="50dp" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="4dp"
                    android:layout_centerVertical="true"
                    android:layout_below="@id/test1"
                    android:textSize="12sp"
                    android:text="Test Account"
                    android:layout_marginLeft="30dp" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/relativeLayout"
                android:layout_width="120dp"
                android:layout_height="110dp"
                android:layout_alignBottom="@+id/relativeLayout3"
                android:layout_alignLeft="@+id/relativeLayout5"
                android:layout_alignStart="@+id/relativeLayout5"
                android:layout_marginBottom="50dp"
                android:backgroundTint="#138D75"
                android:background="@drawable/ic_hecta"></RelativeLayout>
            <RelativeLayout
                android:layout_width="120dp"
                android:layout_height="110dp"
                android:layout_centerHorizontal="true"
                android:layout_above="@id/relativeLayout3"
                android:layout_marginBottom="5dp"
                android:backgroundTint="#117A65"
                android:background="@drawable/ic_hecta"
                android:id="@+id/relativeLayout2"></RelativeLayout>


            <RelativeLayout
                android:id="@+id/relativeLayout4"
                android:layout_width="120dp"
                android:layout_height="110dp"
                android:layout_alignBottom="@+id/relativeLayout5"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginBottom="2dp"
                android:layout_marginLeft="33dp"
                android:layout_marginStart="36dp"
                android:background="@drawable/ic_hecta"
                android:backgroundTint="#73c6b6"></RelativeLayout>



            <RelativeLayout
                android:layout_width="120dp"
                android:layout_height="110dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_alignTop="@+id/relativeLayout3"
                android:layout_marginLeft="100dp"
                android:layout_marginStart="124dp"
                android:layout_marginTop="105dp"
                android:backgroundTint="#45B39D"
                android:background="@drawable/ic_hecta"
                ></RelativeLayout>

            <RelativeLayout
                android:layout_width="120dp"
                android:layout_height="110dp"
                android:layout_above="@id/relativeLayout5"
                android:layout_alignLeft="@+id/relativeLayout4"
                android:layout_marginBottom="-46.25dp"
                android:layout_marginLeft="2dp"
                android:background="@drawable/ic_hecta"
                android:backgroundTint="#0e6655" />

        </RelativeLayout>
        <!--<include  layout="@layout/activity_test_"-->
            <!--android:layout_height="400dp"-->
            <!--android:layout_width="match_parent"-->
            <!--android:id="@+id/starts"/>-->
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="50dp">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="40sp"
                android:textAlignment="center"
                android:layout_centerHorizontal="true"
                android:text="OutStanding"
                android:fontFamily="@font/avenirltstd_medium"/>
        </RelativeLayout>
        <include  layout="@layout/activity_test_"
            android:layout_height="600dp"
            android:layout_width="match_parent"
            android:id="@+id/star"/>
    </LinearLayout>
</ScrollView>
